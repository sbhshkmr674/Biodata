<div class="form-wrappor">
<%= form_for @info, html: { class: 'form-horizontal', multipart: true } do |f|%>
    <center>
	<div class="col-md-8 col-md-offset-2">
              <div class="header-text">
                  <h2>Enter Your Details</h2>
                  <hr />
              </div>
          </div>
	<div class="form-group">
		<div class="col-span-3">
	    	FirstName*
	    </div>
	    <div class="col-span-9">
	    	<%= f.text_field :f_name, placeholder: 'Enter Your First name', require: true%>
	    </div>
	</div>
	<div class="form-group">
		<div class="col-span-3">
	    	LastName*
	    </div>
	    <div class="col-span-9">
	    	<%= f.text_field :l_name, placeholder: 'Enter Your Last Name', require: true%>
	    </div>
	</div> 
	<div class="form-group">
		<div class="col-span-3">
	    	DOB*
	    </div>
	    <div class="col-span-9" id="calendar">
	    	<%= f.date_field :dob, placeholder: 'Enter Your DOB', require: true%>
	    </div>
	</div> 
	
	<div class="form-group">
		<div class="col-span-3">
	    	Country*
	    </div>
	    <div class="col-span-9">
               <%= f.country_select :country, priority: %w(US CA), prompt: 'Please select a country' %>
	    </div>
	</div>
	<div class="form-group">
		<div class="col-span-3">
	    	State*
	    </div>
	    <div class="col-span-9">
           <%= render partial: 'subregion_select', locals: {parent_region: f.object.country} %>

        </div>
	</div>
	<div class="form-group">
		<div class="col-span-3">
	    	Distric*
	    </div>
	    <div class="col-span-9">
	    	<%= f.text_field :distric %>
	    </div>
	</div>
	<div class="form-group">
		<div class="col-span-3">
	    	Zip Code
	    </div>
	    <div class="col-span-9">
	    	<%= f.text_field :zip_code %>
	    </div>
	</div>
	<div class="col-md-8">
  		<%= f.button "Save and Continue", class: "btn btn-success-green"%>
    </div>
    </center>
    <% end %>
