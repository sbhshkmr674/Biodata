<div class="form-wrapper">
	<%= form_for @education do |f| %>
	    <div class="field">
	   		<%= f.label :y_passout, "Enter Passout Year"%>
	   		<%= f.text_field :y_passout, :placeholder => "Enter Your Passout Year ", require: true%>
	   	</div>
	   	<div class="field">
	   		<%= f.label :s_name, "School Name"%>
	   		<%= f.text_field :s_name, :placeholder => "Enter Your 10th details ", require: true%>
	   	</div>
	   	<div class="field">
	   		<%= f.label :b_name, "Enter Board Name"%>
	   		<%= f.text_field :b_name, :placeholder => "From which board You Passout ", require: true%>
	   	</div>
	   	<div class="field">
	   		<%= f.label :o_marks, "Enter Obtained Marks"%>
	   		<%= f.text_field :o_marks, :placeholder => "Enter Your Obtained Marks ", require: true%>
	   	</div>
	   	<div class="field">
	   		<%= f.label :division, "Enter Your Division"%>
	   		<%= f.text_field :division, :placeholder => "Which Division You Got ", require: true%>
	   	</div>

	   	<div class="col-md-9">
	   	     <%= f.button :Submit , class:"btn btn-success" %>
	   	</div>
	<% end %>
</div>     	
